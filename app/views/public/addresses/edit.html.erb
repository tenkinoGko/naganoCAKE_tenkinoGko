<h1>配送先編集</h1>


<%= form_with model: @address do |f| %>
  <div class="form-group">
    郵便番号(ハイフンなし)　<%= f.text_field :post_code, class: 'form-control address_post_code' %>
  </div>
  <div class="form-group">
    住所　<%= f.text_field :address, class: 'form-control address_address' %>
  </div>
  <div class="form-group">
    宛名　<%= f.text_field :name, class: 'form-control address_name' %>
  </div>
  <div class="form-group">
    <%= f.submit '変更内容を保存', class: 'btn btn-success' %>
  </div>
<% end %>

